{
    "name": "madmatt/silverstripe-iplists",
    "type": "silverstripe-vendormodule",
    "description": "Create allow and deny lists based on IP addresses",
    "license": "MIT",

    "require": {
        "php": "^8.1",
        "silverstripe/framework": "^5",
        "silverstripe/admin": "^2",
        "symbiote/silverstripe-gridfieldextensions": "^4",
        "s1lentium/iptools": "^1.1",
        "silverstripe/auditor": "^3",
        "silverstripe/errorpage": "^2"
    },

    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3"
    },

    "autoload": {
        "psr-4": {
            "Madmatt\\IPLists\\": "src/",
            "Madmatt\\IPLists\\Tests\\": "tests/"
        }
    },

    "keywords": [
        "silverstripe",
        "CMS",
        "security",
        "iplists",
        "ip",
        "allowlist",
        "denylist"
    ]
}
